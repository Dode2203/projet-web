{% extends 'base.html.twig' %}

{% block title %}Proprietaire index{% endblock %}

{% block body %}
    <h1>Proprietaire index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>IdProprietaire</th>
                <th>Nom</th>
                <th>NumTelephone</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for proprietaire in proprietaires %}
            <tr>
                <td>{{ proprietaire.idProprietaire }}</td>
                <td>{{ proprietaire.nom }}</td>
                <td>{{ proprietaire.numTelephone }}</td>
                <td>
                    <a href="{{ path('app_proprietaire_show', {'idProprietaire': proprietaire.idProprietaire}) }}">show</a>
                    <a href="{{ path('app_proprietaire_edit', {'idProprietaire': proprietaire.idProprietaire}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_proprietaire_new') }}">Create new</a>
{% endblock %}
